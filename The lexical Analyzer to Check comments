#include <stdio.h>

int main() {
    char line[100];
    fgets(line, 100, stdin);

    if (line[0] == '/' && line[1] == '/')
        printf("Single line comment\n");
    else if (line[0] == '/' && line[1] == '*')
        printf("Multi-line comment\n");
    else
        printf("Not a comment\n");

    return 0;
}
